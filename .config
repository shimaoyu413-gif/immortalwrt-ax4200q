# Target: MediaTek Filogic (ASUS TUF-AX4200Q)
CONFIG_TARGET_mediatek=y
CONFIG_TARGET_mediatek_filogic=y
CONFIG_TARGET_mediatek_filogic_DEVICE_asus_tuf-ax4200q=y

# Basic system
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-mod-system=y
CONFIG_PACKAGE_luci-mod-network=y
CONFIG_PACKAGE_luci-mod-status=y
CONFIG_PACKAGE_luci-theme-material=y
CONFIG_LUCI_LANG_zh_Hans=y

# IPv6 support (essential only)
CONFIG_IPV6=y
CONFIG_PACKAGE_odhcp6c=y          # DHCPv6 client
CONFIG_PACKAGE_odhcpd-ipv6only=y  # Router Advertisement + DHCPv6 server
CONFIG_PACKAGE_ipv6helper=y       # Helper for IPv6 setup in LuCI

# LAN5 (2.5G port) support
CONFIG_PACKAGE_kmod-dsa-mt753x=y
CONFIG_PACKAGE_kmod-mdio=y
CONFIG_PACKAGE_kmod-libphy=y
CONFIG_PACKAGE_swconfig=y

# Wireless & USB (keep minimal)
CONFIG_PACKAGE_kmod-usb3=y
CONFIG_PACKAGE_kmod-mt7915e=y

# Disable heavy/bloat packages
# Python
# CONFIG_PACKAGE_python3 is not set
# CONFIG_PACKAGE_python3-pip is not set

# Perl
# CONFIG_PACKAGE_perl is not set

# IPv6 tunneling (not needed for most users)
# CONFIG_PACKAGE_6in4 is not set
# CONFIG_PACKAGE_6rd is not set
# CONFIG_PACKAGE_6to4 is not set

# Debug & development
# CONFIG_KERNEL_DEBUG is not set
# CONFIG_PACKAGE_gdb is not set
# CONFIG_PACKAGE_strace is not set

# Telephony, multimedia, etc.
# CONFIG_FEED_telephony is not set
# CONFIG_PACKAGE_ffmpeg is not set
# CONFIG_PACKAGE_vlc is not set
